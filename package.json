{
  "name": "oshirasekun",
  "version": "0.0.0",
  "description": "Message and Login Form for Client",
  "main": "dst/main.js",
  "scripts": {
    "buildtsc": "tsc",
    "buildpug": "pug -o dst/ src/",
    "buildsass": "node-sass -o dst/ -r src/",
    "build": "npm run buildtsc && npm run buildpug && npm run buildsass",
    "prestart": "npm run build",
    "start": "electron .",
    "package": "electron-packager . --out=pkg --ignore=src --ignore=pkg",
    "clean": "rimraf dst",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "raccy",
  "license": "MIT",
  "devDependencies": {
    "@types/electron": "^1.4.25",
    "@types/node": "^6.0.46",
    "electron": "^1.4.8",
    "electron-packager": "^8.3.0",
    "node-sass": "^3.13.0",
    "pug": "^2.0.0-beta6",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.5.4",
    "typescript": "^2.0.10"
  }
}
