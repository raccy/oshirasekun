{
  "name": "oshirasekun",
  "version": "0.0.0",
  "description": "Message and Login Form for Client",
  "main": "dst/main.js",
  "scripts": {
    "buildcoffee": "coffee -b -c -t -o dst/ src/",
    "buildpug": "pug -o dst/ src/",
    "buildsass": "node-sass --include-path node_modules/bootstrap/scss --include-path node_modules/font-awesome/scss -o dst/ -r src/",
    "buildfont": "copyup -f node_modules/font-awesome/fonts/* dst/fonts",
    "build": "npm run buildcoffee && npm run buildpug && npm run buildsass && npm run buildfont",
    "prestart": "npm run build",
    "start": "electron .",
    "debug": "cross-env NODE_ENV=development electron . -d -c config/oshirasekun.yml",
    "package": "npm run build && electron-packager . --out=pkg --ignore=src --ignore=pkg",
    "clean": "rimraf dst && rimraf pkg",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "raccy",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "coffeescript": "^2.0.1",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.0.5",
    "electron": "^1.7.8",
    "electron-packager": "^9.1.0",
    "font-awesome": "^4.7.0",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-beta6",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta",
    "bootstrap.native": "^2.0.17",
    "handlebars": "^4.0.10",
    "iconv-lite": "^0.4.19",
    "jquery": "^3.2.1",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "node-getopt": "^0.2.3",
    "popper.js": "^1.12.5",
    "ramda": "^0.24.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-electron-store": "^0.4.1",
    "redux-form": "^7.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
}
