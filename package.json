{
  "name": "oshirasekun",
  "version": "0.0.0",
  "description": "Message and Login Form for Client",
  "main": "dst/main.js",
  "scripts": {
    "buildcoffee": "coffee -b -c -t -m -o dst/ src/",
    "buildtsc": "tsc",
    "buildpug": "pug -o dst/ src/",
    "buildsass": "node-sass --include-path node_modules/bootstrap/scss --include-path node_modules/font-awesome/scss -o dst/ -r src/",
    "buildfont": "copyup -f node_modules/font-awesome/fonts/* dst/fonts",
    "build": "npm run buildtsc && npm run buildpug && npm run buildsass && npm run buildfont",
    "prestart": "npm run build",
    "start": "electron .",
    "debug": "electron . -d -c config/oshirasekun.yml",
    "package": "npm run build && electron-packager . --out=pkg --ignore=src --ignore=pkg",
    "clean": "rimraf dst && rimraf pkg",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "raccy",
  "license": "MIT",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/handlebars": "^4.0.31",
    "@types/iconv-lite": "^0.0.1",
    "@types/jquery": "^2.0.39",
    "@types/js-yaml": "^3.5.29",
    "@types/marked": "^0.0.28",
    "@types/node": "^6.0.51",
    "@types/node-getopt": "^0.2.29",
    "@types/ramda": "0.0.2",
    "@types/react": "^16.0.7",
    "@types/react-dom": "^15.5.5",
    "@types/react-redux": "^5.0.9",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.2.2",
    "@types/redux-form": "^7.0.5",
    "@types/redux-logger": "^3.0.4",
    "@types/redux-thunk": "^2.1.0",
    "@types/tether": "^1.1.27",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "coffeescript": "^2.0.1",
    "copyfiles": "^1.2.0",
    "electron": "^1.7.8",
    "electron-packager": "^9.1.0",
    "font-awesome": "^4.7.0",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-beta6",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.6.2",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "handlebars": "^4.0.6",
    "iconv-lite": "^0.4.15",
    "js-yaml": "^3.7.0",
    "marked": "^0.3.6",
    "node-getopt": "^0.2.3",
    "ramda": "^0.23.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-electron-store": "^0.4.1",
    "redux-form": "^7.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
}
