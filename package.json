{
  "name": "oshirasekun",
  "version": "0.0.0",
  "description": "Message and Login Form for Client",
  "main": "dst/main.js",
  "scripts": {
    "buildtsc": "tsc",
    "buildpug": "pug -o dst/ src/",
    "buildsass": "node-sass --include-path node_modules/bootstrap/scss --include-path node_modules/font-awesome/scss -o dst/ -r src/",
    "buildfont": "copyup -f node_modules/font-awesome/fonts/* dst/fonts",
    "build": "npm run buildtsc && npm run buildpug && npm run buildsass && npm run buildfont",
    "prestart": "npm run build",
    "start": "electron .",
    "debug": "electron . -d -c config/oshirasekun.yml",
    "package": "npm run build && electron-packager . --out=pkg --ignore=src --ignore=pkg",
    "clean": "rimraf dst && rimraf pkg",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "raccy",
  "license": "MIT",
  "devDependencies": {
    "@types/electron": "1.4.28",
    "@types/handlebars": "^4.0.31",
    "@types/jquery": "^2.0.34",
    "@types/js-yaml": "^3.5.29",
    "@types/marked": "^0.0.28",
    "@types/node": "^6.0.51",
    "@types/node-getopt": "^0.2.29",
    "@types/ramda": "0.0.2",
    "@types/react": "^0.14.50",
    "@types/react-dom": "^0.14.19",
    "@types/react-redux": "^4.4.35",
    "@types/redux": "^3.6.31",
    "@types/redux-actions": "^1.2.2",
    "@types/redux-form": "^6.3.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/tether": "^1.1.27",
    "copyfiles": "^1.0.0",
    "electron": "^1.4.13",
    "electron-packager": "^8.3.0",
    "font-awesome": "^4.7.0",
    "node-sass": "^3.13.0",
    "pug": "^2.0.0-beta6",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.5.4",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.5",
    "handlebars": "^4.0.6",
    "js-yaml": "^3.7.0",
    "marked": "^0.3.6",
    "node-getopt": "^0.2.3",
    "ramda": "^0.22.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-electron-store": "^0.4.1",
    "redux-form": "^6.4.3",
    "redux-thunk": "^2.1.0",
    "windows-fs": "^1.2.0"
  }
}
